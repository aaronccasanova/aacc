{
  "name": "plop-dir",
  "version": "0.2.0",
  "description": "Directory based file generator",
  "author": "Aaron Casanova <aaronccasanova@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "npm-run-all --parallel 'build:* -- --watch'",
    "build": "npm-run-all --parallel 'build:*'",
    "build:js": "rollup -c",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "TIMING=1 eslint . --ext .js,.jsx,.ts,.tsx,.cjs,.mjs --cache",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "plop": "plop"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "plop": "^3.0.0"
  },
  "devDependencies": {
    "@aacc/babel-preset": "*",
    "@aacc/browserslist-config": "*",
    "@aacc/eslint-config": "*",
    "@aacc/tsconfigs": "*",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^28.1.2",
    "babel-jest": "^28.1.1",
    "jest": "^28.1.1",
    "plop": "^3.0.0",
    "rollup": "^4.9.4",
    "type-fest": "^2.14.0",
    "typescript": "~5.3.3"
  },
  "browserslist": [
    "extends @aacc/browserslist-config"
  ],
  "publishConfig": {
    "access": "public",
    "@aacc:registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronccasanova/aacc.git",
    "directory": "packages/plop-dir"
  },
  "bugs": {
    "url": "https://github.com/aaronccasanova/aacc/issues"
  },
  "homepage": "https://github.com/aaronccasanova/aacc/blob/main/packages/plop-dir/README.md"
}
